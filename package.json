{
  "name": "mock-phone-preview",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.0.0",
  "publisher": "TatsuyaKosuge",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "mobile",
    "mobile-preview",
    "mobile-testing",
    "mobile-development",
    "smartphone",
    "phone",
    "device",
    "device frame",
    "preview",
    "html",
    "css",
    "responsive",
    "iphone",
    "android",
    "simulator",
    "qr",
    "qrcode",
    "live-preview"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/majikarikeruo/mock-phone-preview"
  },
  "bugs": {
    "url": "https://github.com/majikarikeruo/mock-phone-preview/issues"
  },
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e293b",
    "theme": "dark"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mockPhonePreview.start",
        "title": "%command.start%"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "mockPhonePreview.start",
          "when": "resourceLangId == html",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "mockPhonePreview.start",
          "when": "resourceLangId == html",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch --mode development",
    "package": "webpack && vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/qrcode": "^1.5.5",
    "@types/vscode": "^1.86.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.4.5",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "qrcode": "^1.5.4"
  }
}
